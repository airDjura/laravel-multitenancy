{
    "name": "airdjura/laravel-multitenancy",
    "description": "A simple, single database multi-tenancy solution for Laravel",
    "version": "3.0.5",
    "homepage": "https://github.com/airDjura/laravel-multitenancy",
    "license": "MIT",
    "keywords": [
        "tenant",
        "tenancy",
        "multitenant",
        "multitenancy",
        "laravel"
    ],
    "authors": [
        {
            "name": "airDjura",
            "email": "airdjura@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "illuminate/support": "5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|6.0.*|7.0.*|8.0.*|8.43.0|9.*"
    },
    "require-dev": {
        "mockery/mockery": "~0.9|~1.0",
        "phpunit/phpunit": "5.5.*|6.0.*|7.0.*",
        "laravel/framework": "5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|6.0.*|7.0.*|8.0.*|8.43.0|9.*"
    },
    "autoload": {
        "psr-4": {
            "airDjura\\Landlord\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "airDjura\\Landlord\\LandlordServiceProvider"
            ],
            "aliases": {
                "Landlord": "airDjura\\Landlord\\Facades\\Landlord"
            }
        }
    }
}
